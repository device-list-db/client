<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="350.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.holo.gui.BookController">
   <children>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
         <children>
            <TableView fx:id="tableView" prefHeight="300.0" prefWidth="600.0">
               <columns>
                  <TableColumn fx:id="title" prefWidth="75.0" text="Title" />
                  <TableColumn fx:id="author" prefWidth="75.0" text="Author" />
                  <TableColumn fx:id="isbn" prefWidth="98.4000244140625" text="ISBN Number" />
                  <TableColumn fx:id="rentee" prefWidth="98.4000244140625" text="Rentee" />
                  <TableColumn fx:id="dueDate" prefWidth="98.4000244140625" text="Due Date" />
               </columns>
            </TableView>
            <ButtonBar layoutX="97.0" layoutY="303.0" prefHeight="27.0" prefWidth="417.0">
               <buttons>
                  <Button disable="true" mnemonicParsing="false" prefHeight="25.0" prefWidth="96.0" text="Return Book" />
                  <Button defaultButton="true" minWidth="58.0" mnemonicParsing="false" onAction="#addBook" prefHeight="13.0" prefWidth="78.0" text="Add Book" />
                  <Button disable="true" mnemonicParsing="false" text="Rent Book" />
                  <Button cancelButton="true" mnemonicParsing="false" onAction="#backPressed" text="Back" />
               </buttons>
            </ButtonBar>
         </children>
      </AnchorPane>
   </children>
</VBox>
